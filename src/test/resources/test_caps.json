{
  "instances": {
    "public": {
      "url": "https://bstackdemo.com/"
    },
    "private": {
      "url": "http://bs-local.com:3000/"
    },
    "private1": {
      "url": "http://bs-local.com:3001/"
    }
  },
  "common-capabilities": {
    "project": "BrowserStack Framework Examples",
    "build": "JUnit 5",
    "browserstack.debug": "true",
    "browserstack.networkLogs": "true",
    "browserstack.console": "verbose"
  },
  "profile-capabilities": {
    "on-premise": {
      "capabilities": {"browser": "Chrome"},
      "instance": "public",
      "drivers": {"webdriver.gecko.driver":"/opt/libs/drivers/geckodriver",
                  "webdriver.chrome.driver":"/opt/libs/drivers/chromedriver"}
    },
    "docker": {"capabilities": {"browser": "Firefox"},
      "instance": "public"},
    "single": {
      "capabilities": {
        "name": "Single Test: "
      },
      "instance": "public"
    },
    "local": {
      "capabilities": {
        "name": "Local Test: "
      },
      "instance": "private"
    },
    "parallel": {
      "capabilities": {
        "name": "Parallel Test: "
      },
      "instance": "public"
    },
    "local-parallel": {
      "capabilities": {
        "name": "Local Parallel Test: "
      },
      "instance": "private"
    }
  },
  "local-options": {
    "force": "true",
    "forceLocal": "true",
    "verbose": "true",
    "logFile": "parallel_local_test.log"
  }
}